{% extends 'baseContent.html.twig' %}

{% block content %}
    <table>
        <h2>Gebruikers</h2>
        <thead>
        <tr>
            <th>Gebruikersnaam</th>
            <th>Email</th>
        </tr>
        </thead>

        <tbody>
        {% for groupAssociation in groupsAssociations %}
            <tr>
                <td>{{ groupAssociation.user.username }}</td>
                <td>{{ groupAssociation.user.email }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}